{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "styles/_Global/index.js",
    "_Components/MarkdownRenderer/index.js",
    "_Components/Comment/index.ㅓㄴㅌ",
    "_Components/Comment/index.jsx",
    "_Components/Comment/index.js"
  ],
  "exclude": ["node_modules"],
}
